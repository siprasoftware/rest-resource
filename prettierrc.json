{
    "parser": "vue",
    "printWidth": 200,
    "trailingComma": "es5",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "arrowParens": "always",
    "tabWidth": 4,
    "semi": false,
    "singleQuote": true,
    "useTabs": false,
    "overrides": [
        {
            "files": "*.js",
            "options": {
                "parser": "babel"
            }
        },
        {
            "files": [
                "*.json",
                ".prettierrc"
            ],
            "options": {
                "parser": "json"
            }
        }
    ]
}